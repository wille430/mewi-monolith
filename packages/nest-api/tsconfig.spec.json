{
    "extends": "./tsconfig.json",
    "include": [
        "**/*.spec.ts",
        "**/*.test.ts",
        "**/*.test.tsx",
        "**/*.spec.tsx",
        "src/**/*",
        "**/__mocks__/*"
    ],
    "compilerOptions": {
        "paths": {
            "@/*": ["./src/*"]
        },
        "composite": true
    }
}
