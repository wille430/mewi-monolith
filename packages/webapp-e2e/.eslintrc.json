{
    "extends": ["plugin:cypress/recommended", "../../.eslintrc.json"],
    "ignorePatterns": ["!**/*"],
    "overrides": [
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "rules": {
                "@typescript-eslint/no-explicit-any": "off",
                "cypress/no-unnecessary-waiting": 0,
                "@nrwl/nx/enforce-module-boundaries": 0
            }
        }
    ]
}
