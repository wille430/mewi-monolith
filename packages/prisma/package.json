{
    "name": "@mewi/prisma",
    "version": "1.0.1",
    "description": "Shared prisma schema",
    "repository": {
        "type": "git",
        "url": "git://github.com/wille430/mewi-monolith.git"
    },
    "exports": {
        ".": "./dist/client/index.js",
        "./index-browser": "./dist/client/index-browser.js",
        "./factory": "./dist/client/factory/index.js"
    },
    "typesVersions": {
        "*": {
            "*": [
                "./dist/client/index.d.ts"
            ],
            "index-browser": [
                "./dist/client/index.d.ts"
            ],
            "factory": [
                "./dist/client/factory/index.d.ts"
            ]
        }
    },
    "scripts": {
        "preinstall": "npm run prisma:generate",
        "prisma:generate": "npx dotenv -- npx prisma generate --schema ./src/schema.prisma",
        "prisma:seed": "npx dotenv -- prisma db seed"
    },
    "author": "Mewi",
    "license": "ISC",
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts",
        "schema": "./dist/client/schema.prisma"
    },
    "devDependencies": {
        "@types/node": "^17.0.31",
        "ts-node": "^10.7.0"
    },
    "dependencies": {
        "@faker-js/faker": "^6.3.1",
        "@prisma/client": "3.14.0",
        "dotenv-cli": "^5.1.0",
        "prisma": "3.13.0",
        "prisma-factory": "^0.1.12",
        "typescript": "^4.6.4"
    }
}