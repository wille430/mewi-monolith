{
    "name": "@mewi/prisma",
    "version": "1.0.1",
    "description": "Shared prisma client",
    "exports": {
        ".": "./dist/index.js",
        "./index-browser": "./dist/index-browser.js"
    },
    "types": "./dist/index.d.ts",
    "main": "./dist/index.js",
    "repository": {
        "type": "git",
        "url": "git://github.com/wille430/mewi-monolith.git"
    },
    "scripts": {
        "build": "microbundle -f cjs src/*.ts --no-compress",
        "dev": "microbundle watch",
        "pack": "pnpm pack --pack-destination ../../archive",
        "prepare": "npm run prisma:generate && npm run build",
        "postinstall": "npm run prisma:generate",
        "prisma:generate": "dotenv -- prisma generate"
    },
    "author": "Mewi",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^3.13.0",
        "prisma-factory": "^0.1.12"
    },
    "devDependencies": {
        "dotenv-cli": "^5.1.0",
        "microbundle": "^0.14.2",
        "prisma": "^3.13.0"
    },
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
    }
}