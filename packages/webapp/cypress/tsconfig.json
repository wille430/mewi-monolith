        "./support/*.ts",
        "./integration/*.ts",
        "**/*.ts",
        "../src/*"
    ],
    "compilerOptions": {
        "sourceMap": false,
        "types": ["cypress", "node", "cypress-real-events"],
        "strict": true,
        "baseUrl": "../src",
        "target": "ES6"
    }
}
