{
    "root": "packages/webapp",
    "sourceRoot": "packages/webapp/src",
    "projectType": "application",
    "targets": {
        "deploy": {
            "builder": "@netlify-builder/deploy:deploy",
            "options": {
                "outputPath": "dist/packages/webapp",
                "siteId": "09e54bfa-3c3b-4034-9e90-2372d737a51b"
            }
        },
        "build": {
            "executor": "@nxext/vite:build",
            "outputs": ["{options.outputPath}"],
            "defaultConfiguration": "production",
            "options": {
                "outputPath": "dist/package/webapp",
                "baseHref": "/",
                "configFile": "@nxext/vite/plugins/vite",
                "frameworkConfigFile": "packages/webapp/vite.config.ts"
            },
            "configurations": {
                "production": {}
            }
        },
        "serve": {
            "executor": "@nxext/vite:dev",
            "options": {
                "outputPath": "dist/package/webapp",
                "baseHref": "/",
                "configFile": "packages/webapp/vite.config.ts",
                "frameworkConfigFile": "@nxext/vite/plugins/vite"
            },
            "configurations": {
                "production": {}
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": ["{options.outputFile}"],
            "options": {
                "lintFilePatterns": ["packages/webapp/**/*.{ts,tsx,js,jsx}"]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": ["coverage/packages/webapp"],
            "options": {
                "jestConfig": "packages/webapp/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "storybook": {
            "executor": "@nrwl/storybook:storybook",
            "options": {
                "uiFramework": "@storybook/react",
                "port": 4400,
                "config": {
                    "configFolder": "packages/webapp/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@nrwl/storybook:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "uiFramework": "@storybook/react",
                "outputPath": "dist/storybook/webapp",
                "config": {
                    "configFolder": "packages/webapp/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    },
    "tags": []
}
