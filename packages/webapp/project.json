{
    "root": "packages/webapp",
    "sourceRoot": "packages/webapp/src",
    "projectType": "application",
    "targets": {
        "deploy": {
            "builder": "@netlify-builder/deploy:deploy",
            "options": {
                "outputPath": "dist/packages/webapp",
                "siteId": "09e54bfa-3c3b-4034-9e90-2372d737a51b"
            }
        },
        "build": {
            "executor": "@nxext/vite:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/packages/webapp",
                "baseUrl": ".",
                "assets": [
                    "./src/favicon.ico",
                    "packages/webapp/src/assets",
                    "packages/webapp/src/_redirects"
                ]
            },
            "defaultConfiguration": "production",
            "configFile": "packages/webapp/vite.config.ts",
            "configurations": {
                "production": {
                    "fileReplacements": [
                        {
                            "replace": "packages/webapp/src/environments/environment.ts",
                            "with": "packages/webapp/src/environments/environment.prod.ts"
                        }
                    ],
                    "optimization": true,
                    "outputHashing": "all",
                    "sourceMap": false,
                    "namedChunks": false,
                    "extractLicenses": true,
                    "vendorChunk": false
                }
            }
        },
        "serve": {
            "executor": "@nxext/vite:dev",
            "options": {
                "buildTarget": "webapp:build",
                "hmr": true
            },
            "configFile": "packages/webpack/vite.config.ts",
            "configurations": {
                "production": {
                    "buildTarget": "webapp:build:production",
                    "hmr": false
                }
            }
        },
        "lint": {
            "executor": "@nrwl/linter:eslint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/webapp/**/*.{ts,tsx,js,jsx}"
                ]
            }
        },
        "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
                "coverage/packages/webapp"
            ],
            "options": {
                "jestConfig": "packages/webapp/jest.config.ts",
                "passWithNoTests": true
            }
        },
        "storybook": {
            "executor": "@nrwl/storybook:storybook",
            "options": {
                "uiFramework": "@storybook/react",
                "port": 4400,
                "config": {
                    "configFolder": "packages/webapp/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        },
        "build-storybook": {
            "executor": "@nrwl/storybook:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "uiFramework": "@storybook/react",
                "outputPath": "dist/storybook/webapp",
                "config": {
                    "configFolder": "packages/webapp/.storybook"
                }
            },
            "configurations": {
                "ci": {
                    "quiet": true
                }
            }
        }
    },
    "tags": []
}