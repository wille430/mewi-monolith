{
    "name": "@mewi/schemas",
    "version": "1.0.1",
    "description": "Shared prisma schema",
    "repository": {
        "type": "git",
        "url": "git://github.com/wille430/mewi-monolith.git"
    },
    "scripts": {
        "postinstall": "npm run prisma:generate",
        "prisma:generate": "dotenv -- prisma generate",
        "prisma:seed": "dotenv -- prisma db seed"
    },
    "author": "Mewi",
    "license": "ISC",
    "prisma": {
        "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./prisma/seed.ts"
    },
    "devDependencies": {
        "dotenv-cli": "^5.1.0",
        "prisma": "^3.13.0",
        "prisma-factory": "^0.1.12",
        "ts-node": "^10.7.0"
    },
    "dependencies": {
        "@faker-js/faker": "^6.3.1",
        "@prisma/client": "^3.13.0"
    }
}
