{
    "name": "@mewi/common",
    "version": "1.0.0",
    "description": "Shared package for Mewi monorepo",
    "main": "dist/index",
    "types": "dist/index.d.ts",
    "exports": {
        ".": "./dist/index.js",
        "./types": "./dist/types/index.js",
        "./error": "./dist/error/index.js",
        "./utils": "./dist/utils/index.js"
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ],
            "types": [
                "dist/types/index.d.ts"
            ],
            "error": [
                "dist/error/index.d.ts"
            ],
            "utils": [
                "dist/utils/index.d.ts"
            ]
        }
    },
    "scripts": {
        "build": "npx tsc",
        "build:watch": "npx tsc -w",
        "pack": "pnpm pack --pack-destination ../../archive",
        "publish": "pnpm publish"
    },
    "author": "Mewi",
    "license": "ISC",
    "dependencies": {
        "@faker-js/faker": "^6.0.0",
        "@types/lodash": "^4.14.180",
        "lodash": "^4.17.21",
        "mongoose": "6.2.9",
        "query-string": "^7.1.1",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^1.0.7",
        "vitest": "^0.8.1"
    }
}
