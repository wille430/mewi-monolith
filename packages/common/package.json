{
    "name": "@wille430/common",
    "version": "1.0.1",
    "description": "Shared package for Mewi monorepo",
    "source": "src/index.ts",
    "exports": {
        "require": "./dist/index.js",
        "default": "./dist/index.modern.js"
    },
    "main": "dist/index.js",
    "module": "dist/index.module.js",
    "types": "dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "git://github.com/wille430/mewi-monolith.git"
    },
    "files": [
        "dist/**/*"
    ],
    "scripts": {
        "build": "mkdir -p ./node_modules/@mewi/prisma && cp ../../node_modules/@mewi/prisma/* ./node_modules/@mewi/prisma/ -r && npx microbundle build",
        "dev": "npx microbundle watch",
        "pack": "pnpm pack --pack-destination ../../archive",
        "publish": "pnpm publish --no-git-checks",
        "prepare": "npm run build"
    },
    "publishConfig": {
        "@wille430:registry": "https://npm.pkg.github.com"
    },
    "author": "Mewi",
    "license": "ISC",
    "dependencies": {
        "@faker-js/faker": "^6.0.0",
        "lodash": "^4.17.21",
        "query-string": "^7.1.1",
        "tslib": "^2.3.1",
        "typescript": "^4.6.4"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.180",
        "@vitejs/plugin-react": "^1.0.7",
        "microbundle": "^0.14.2",
        "vitest": "^0.8.1"
    }
}
