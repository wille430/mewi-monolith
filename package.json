{
    "name": "mewi",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "lint": "eslint packages/**/*/src/**/*.{ts,tsx}",
        "lint:fix": "eslint packages/**/*/src/**/*.{ts,tsx} --fix",
        "e2e": "pnpm run e2e -r",
        "build": "pnpm run build -r",
        "test": "pnpm run test -r",
        "format": "prettier -w .",
        "dev": "pnpm run build -F @mewi/util -F @mewi/types & pnpm run dev -r --parallel"
    },
    "private": true,
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.17.0",
        "@typescript-eslint/parser": "^5.17.0",
        "cypress": "^9.5.2",
        "eslint": "^8.12.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "prettier": "^2.6.1"
    },
    "dependencies": {
        "@types/lodash": "^4.14.180",
        "pnpm": "^6.32.3"
    }
}
